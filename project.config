	pushMerge = group user/Crystal ROM (Crystal-Rom)
[access "refs/tags/*"]
	push = +force group user/Crystal ROM (Crystal-Rom)
	pushTag = group user/Crystal ROM (Crystal-Rom)
	pushSignedTag = group user/Crystal ROM (Crystal-Rom)
